{
  "name": "gigantti-gpu-alert",
  "version": "1.0.0",
  "description": "Send alert when Gigantti gets certain GPU's in stock",
  "main": "index.mjs",
  "bin": "bin.mjs",
  "type": "module",
  "engines": {
    "node": "^v14.16.1",
    "npm": "^7.11.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ncc build index.mjs --minify --source-map --target es2020 --external aws-sdk --out dist",
    "predeploy": "npm run test",
    "deploy": "npm run build && npm run package && npm run deploy:tf",
    "deploy:tf": "terraform apply",
    "format": "npm run prettier -- --write && npm run format:tf",
    "format:tf": "terraform fmt",
    "lint": "eslint --ext mjs,cjs,js --ignore-path .gitignore .",
    "package": "zip -jr dist/lambda.zip dist/",
    "prettier": "prettier --ignore-path .gitignore .",
    "start": "node -r dotenv/config bin.mjs",
    "test": "npm run lint && npm run prettier -- --check && npm run format:tf -- -check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hilzu/gigantti-gpu-alert.git"
  },
  "author": "github@me.hilzu.moe",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Hilzu/gigantti-gpu-alert/issues"
  },
  "homepage": "https://github.com/Hilzu/gigantti-gpu-alert#readme",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@vercel/ncc": "^0.28.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "aws-sdk": "^2.892.0",
    "cheerio": "^1.0.0-rc.6",
    "got": "^11.8.2"
  }
}
